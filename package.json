{
  "name": "css-variables",
  "displayName": "CSS Variable",
  "description": "Shows CSS variable values",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:scss"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.96.0",
    "mocha": "^11.0.1",
    "ts-loader": "^9.5.2",
    "typescript": "^4.5.5",
    "webpack": "^5.97.1",
    "webpack-cli": "^4.10.0"
  }
}
